ntainer ,{\n    width: 40%;\n    margin-left: auto;\n    margin-right: auto;\n  }\n  .logo{\n    width: 60%;\n    margin-left: auto;\n    margin-right: auto;\n  }\n  /* Adjust margin and alignment for router link container */\n\n}\n</style>\n\n"],"mappings":"AAOA,eAAe;EACbA,IAAI,EAAE;AACR,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}
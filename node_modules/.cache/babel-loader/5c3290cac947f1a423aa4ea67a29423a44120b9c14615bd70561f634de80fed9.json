{"ast":null,"code":"import Navbar from './Navbar.vue'; // Import the custom navigation bar component\nimport AboutSection from './AboutSection.vue';\nimport contactPage from './ContactPage.vue';\nimport Footer from './Footer.vue';\nexport default {\n  name: 'HomePage',\n  components: {\n    Navbar,\n    // Register the custom navigation bar component\n    AboutSection,\n    contactPage,\n    Footer\n  },\n  data() {\n    return {\n      isExpanded: false,\n      heroMarginLeft: '0',\n      mainContentMarginLeft: '0',\n      belowHeroMarginLeft: '0'\n    };\n  },\n  methods: {\n    toggleMenu(isExpanded) {\n      this.isExpanded = isExpanded;\n      if (this.isExpanded) {\n        this.heroMarginLeft = '125px';\n        this.mainContentMarginLeft = '125px';\n        this.belowHeroMarginLeft = '125px';\n      } else {\n        this.heroMarginLeft = '0';\n        this.mainContentMarginLeft = '0';\n        this.belowHeroMarginLeft = '0';\n      }\n    }\n  }\n};","map":{"version":3,"names":["Navbar","AboutSection","contactPage","Footer","name","components","data","isExpanded","heroMarginLeft","mainContentMarginLeft","belowHeroMarginLeft","methods","toggleMenu"],"sources":["/home/natan/final/front en/src/components/Home.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- Include the custom navigation bar component -->\n    <Navbar @toggleMenu=\"toggleMenu\" :is-expanded=\"isExpanded\" />\n\n\n    <!-- Hero Section -->\n    <section class=\"hero\" :class=\"{ 'pushed': isExpanded }\" :style=\"{ marginLeft: heroMarginLeft }\">\n      <div class=\"hero-content\">\n        <h1>Effortless Contact Management</h1>\n        <p>Simplify your life with our intuitive contact handling platform.</p>\n        <router-link to=\"/sign-up\" class=\"btn btn-primary\">Get Started</router-link>\n      </div>\n    </section>\n\n    <!-- Main Content Area -->\n    <div class=\"main-content\" :class=\"{ 'pushed': isExpanded }\" :style=\"{ marginLeft: mainContentMarginLeft }\">\n      <router-view></router-view>\n    </div>\n\n    <!-- Elements below the hero section -->\n    <div class=\"elements-below-hero\" :class=\"{ 'is-expanded': isExpanded }\" :style=\"{ marginLeft: belowHeroMarginLeft }\">\n      <!-- Content here -->\n      <!-- About Section -->\n      <AboutSection></AboutSection>\n      <contactPage></contactPage>\n      <Footer></Footer>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Navbar from './Navbar.vue'; // Import the custom navigation bar component\nimport AboutSection from './AboutSection.vue';\nimport contactPage from './ContactPage.vue';\nimport Footer from './Footer.vue';\n\nexport default {\n  name: 'HomePage',\n  components: {\n    Navbar, // Register the custom navigation bar component\n    AboutSection,\n    contactPage,\n    Footer,\n  },\n  data() {\n    return {\n      isExpanded: false,\n      heroMarginLeft: '0',\n      mainContentMarginLeft: '0',\n      belowHeroMarginLeft: '0'\n    };\n  },\n  methods: {\n    toggleMenu(isExpanded) {\n      this.isExpanded = isExpanded;\n\n      if (this.isExpanded) {\n        this.heroMarginLeft = '125px';\n        this.mainContentMarginLeft = '125px';\n        this.belowHeroMarginLeft = '125px';\n      } else {\n        this.heroMarginLeft = '0';\n        this.mainContentMarginLeft = '0';\n        this.belowHeroMarginLeft = '0';\n      }\n    }\n  },\n};\n</script>\n\n<style scoped>\n/* Hero section styles */\n.hero {\n   background-image: url('../assets/hero.webp');\n   background-size: cover;\n   background-position: center;\n   color: #ffffff;\n   padding: 100px 0;\n   text-align: center;\n   transition: margin-left 0.3s ease;\n   margin-top: 116px; /* Adjust the margin-top to move the hero section downwards */\n }\n\n.hero-content {\n  max-width: 800px;\n  margin: 0 auto;\n}\n\n.hero-content h1 {\n  font-size: 3rem;\n  margin-bottom: 20px;\n}\n\n.hero-content p {\n  font-size: 1.5rem;\n  margin-bottom: 40px;\n}\n\n/* Button styles */\n.btn {\n  display: inline-block;\n  
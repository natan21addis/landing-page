  isExpanded: false,\n      sidebarWidth: '125px',\n      heroMarginLeft: '0',\n      mainContentMarginLeft: '0',\n      belowHeroMarginLeft: '0'\n    };\n  },\n  methods: {\n    logout() {\n      this.loggedIn = false;\n    },\n    toggleMenu() {\n      this.isExpanded = !this.isExpanded;\n      localStorage.setItem(\"is_expanded\", this.isExpanded.toString());\n\n      if (this.isExpanded) {\n        this.heroMarginLeft = this.sidebarWidth;\n        this.mainContentMarginLeft = this.sidebarWidth;\n        this.belowHeroMarginLeft = this.sidebarWidth;\n      } else {\n        this.heroMarginLeft = '0';\n        this.mainContentMarginLeft = '0';\n        this.belowHeroMarginLeft = '0';\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n.about-section {\n  padding: 50px 0;\n  background-color: #f9f9f9;\n}\n\n.about-section .container {\n  max-width: 960px;\n  margin: 0 auto;\n}\n\n.about-section h2 {\n  font-size: 2em;\n  margin-bottom: 20px;\n}\n\n.about-section p {\n  font-size: 1.2em;\n  line-height: 1.6;\n}\n\n.about-section ul {\n  margin-top: 10px;\n  margin-bottom: 20px;\n}\n\n.about-section ul li {\n  list-style: disc;\n  margin-left: 20px;\n}\n</style>\n\n\n\n@import \"../styles/style_home.css\";\n"],"mappings":"AA6EA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,KAAK;MACfC,UAAU,EAAE,KAAK;MACjBC,YAAY,EAAE,OAAO;MACrBC,cAAc,EAAE,GAAG;MACnBC,qBAAqB,EAAE,GAAG;MAC1BC,mBAAmB,EAAE;IACvB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,MAAMA,CAAA,EAAG;MACP,IAAI,CAACP,QAAO,GAAI,KAAK;IACvB,CAAC;IACDQ,UAAUA,CAAA,EAAG;MACX,IAAI,CAACP,UAAS,GAAI,CAAC,IAAI,CAACA,UAAU;MAClCQ,YAAY,CAACC,OAAO,CAAC,aAAa,EAAE,IAAI,CAACT,UAAU,CAACU,QAAQ,CAAC,CAAC,CAAC;MAE/D,IAAI,IAAI,CAACV,UAAU,EAAE;QACnB,IAAI,CAACE,cAAa,GAAI,IAAI,CAACD,YAAY;QACvC,IAAI,CAACE,qBAAoB,GAAI,IAAI,CAACF,YAAY;QAC9C,IAAI,CAACG,mBAAkB,GAAI,IAAI,CAACH,YAAY;MAC9C,OAAO;QACL,IAAI,CAACC,cAAa,GAAI,GAAG;QACzB,IAAI,CAACC,qBAAoB,GAAI,GAAG;QAChC,IAAI,CAACC,mBAAkB,GAAI,GAAG;MAChC;IACF;EACF;AACF,CAAC","ignoreLis